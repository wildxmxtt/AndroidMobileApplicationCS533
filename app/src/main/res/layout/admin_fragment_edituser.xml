<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".Register">

    <TextView
        android:id="@+id/textView25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/username_hint"
        app:layout_constraintBottom_toTopOf="@+id/admin_productnameupdate_entry_user"
        app:layout_constraintEnd_toEndOf="@+id/admin_productnameupdate_entry_user"
        app:layout_constraintStart_toStartOf="@+id/admin_productnameupdate_entry_user" />

    <TextView
        android:id="@+id/admin_updateItemTitle_textView_user"
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:textStyle="bold"
        android:textColor="@color/black"
        android:text="@string/admin_edituser_text"
        android:textAlignment="center"
        android:textSize="40sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="@string/password_hint"
        app:layout_constraintEnd_toEndOf="@+id/admin_productDescriptionUpdate_entry_user"
        app:layout_constraintStart_toStartOf="@+id/admin_productDescriptionUpdate_entry_user"
        app:layout_constraintTop_toBottomOf="@+id/admin_productnameupdate_entry_user" />

    <EditText
        android:id="@+id/admin_productnameupdate_entry_user"
        android:layout_width="250dp"
        android:layout_height="50dp"
        android:layout_marginTop="20dp"
        android:ems="10"
        android:hint="@string/username_hint"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/admin_updateItemTitle_textView_user" />

    <TextView
        android:id="@+id/textView27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="@string/email_entry"
        app:layout_constraintEnd_toEndOf="@+id/registerEmail_editText_user"
        app:layout_constraintStart_toStartOf="@+id/registerEmail_editText_user"
        app:layout_constraintTop_toBottomOf="@+id/admin_productDescriptionUpdate_entry_user" />

    <EditText
        android:id="@+id/admin_productDescriptionUpdate_entry_user"
        android:layout_width="250dp"
        android:layout_height="50dp"
        android:ems="10"
        android:hint="@string/password_entry"
        android:inputType="textPassword"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView26" />

    <TextView
        android:id="@+id/textView28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="@string/address_entry"
        app:layout_constraintEnd_toEndOf="@+id/registerAddress_editText_user"
        app:layout_constraintStart_toStartOf="@+id/registerAddress_editText_user"
        app:layout_constraintTop_toBottomOf="@+id/registerEmail_editText_user" />

    <EditText
        android:id="@+id/registerEmail_editText_user"
        android:layout_width="250dp"
        android:layout_height="50dp"
        android:ems="10"
        android:hint="@string/email_entry"
        android:inputType="textEmailAddress"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView27" />

    <TextView
        android:id="@+id/textView29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/postcode_entry"
        app:layout_constraintEnd_toEndOf="@+id/registerPostcode_editText_user"
        app:layout_constraintStart_toStartOf="@+id/registerPostcode_editText_user"
        app:layout_constraintTop_toTopOf="@+id/textView28" />

    <EditText
        android:id="@+id/registerAddress_editText_user"
        android:layout_width="250dp"
        android:layout_height="50dp"
        android:layout_marginStart="10dp"
        android:ems="10"
        android:hint="@string/address_entry"
        android:inputType="textPostalAddress"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView28" />

    <TextView
        android:id="@+id/textView30"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="@string/phonenumber_entry"
        app:layout_constraintEnd_toEndOf="@+id/registerPhoneNumber_editText_user"
        app:layout_constraintStart_toStartOf="@+id/registerPhoneNumber_editText_user"
        app:layout_constraintTop_toBottomOf="@+id/registerPostcode_editText_user" />

    <TextView
        android:id="@+id/textView43"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="@string/hobby_entry"
        app:layout_constraintEnd_toEndOf="@+id/admin_updateHobby_editText"
        app:layout_constraintStart_toStartOf="@+id/admin_updateHobby_editText"
        app:layout_constraintTop_toBottomOf="@+id/registerPhoneNumber_editText_user" />

    <EditText
        android:id="@+id/registerPostcode_editText_user"
        android:layout_width="125dp"
        android:layout_height="50dp"
        android:layout_marginEnd="10dp"
        android:ems="10"
        android:hint="@string/postcode_entry"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView29" />

    <EditText
        android:id="@+id/registerPhoneNumber_editText_user"
        android:layout_width="250dp"
        android:layout_height="50dp"
        android:ems="10"
        android:hint="@string/phonenumber_entry"
        android:inputType="phone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView30" />

    <EditText
        android:id="@+id/admin_updateHobby_editText"
        android:layout_width="250dp"
        android:layout_height="100dp"
        android:ems="10"
        android:gravity="start"
        android:hint="@string/hobby_entry"
        android:inputType="textLongMessage|textMultiLine"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView43" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/admin_cancelNewProductUpdate_Button_user"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/baseline_arrow_back_24"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/admin_DeleteUser_Button_user"
        android:layout_width="175dp"
        android:layout_height="50dp"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="75dp"
        android:background="@color/material_dynamic_tertiary50"
        android:text="@string/admin_delete_button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/admin_UpdateProduct_Button_user"
        android:layout_width="175dp"
        android:layout_height="50dp"
        android:layout_marginEnd="10dp"
        android:background="@color/material_dynamic_tertiary50"
        android:text="@string/admin_updateitem_text"
        app:layout_constraintBottom_toBottomOf="@+id/admin_DeleteUser_Button_user"
        app:layout_constraintEnd_toEndOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>